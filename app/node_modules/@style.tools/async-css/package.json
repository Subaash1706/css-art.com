{
    "name": "@style.tools/async-css",
    "version": "1.2.0",
    "description": "A lightweight, IE8+ asynchronous CSS loader that favors performance in modern browsers. For use with üìê Style.Tools CSS optimization browser widget.",
    "author": {
        "name": "info@style.tools",
        "email": "info@style.tools",
        "web": "style.tools"
    },
    "main": "async-css.js",
    "keywords": [
        "async",
        "css",
        "css-loader",
        "asynchronous",
        "localstorage",
        "cache-api",
        "responsive",
        "responsive-css",
        "optimization"
    ],
    "_modules": [
        [
            "async-core",
            "Core async loading functionality. (required)"
        ],
        [
            "event-emitter",
            "Event emitter (required for some modules)"
        ],
        [
            "debug",
            "Debug / Performance timing. (used for dist/debug/ sources)"
        ],
        [
            "css-loader",
            "Async CSS loader."
        ],
        [
            "js-loader",
            "Async script loader."
        ],
        [
            "regex",
            "Regex functionality (required for some modules)"
        ],
        [
            "vendor",
            "Vendor prefixes functionality (required for some modules)"
        ],
        [
            "rebase",
            "Relative URL rebasing / URL compression."
        ],
        [
            "dependency",
            "Dependency based loading."
        ],
        [
            "cache",
            "Core cache functionality."
        ],
        [
            "cache-css",
            "CSS related cache functionality."
        ],
        [
            "cache-js",
            "Javascript related cache functionality."
        ],
        [
            "localstorage",
            "localStorage cache."
        ],
        [
            "cache-api",
            "Cache-API cache."
        ],
        [
            "xhr",
            "XHR request module and CORS proxy."
        ],
        [
            "cache-update",
            "Background cache update (HEAD request via XHR)."
        ],
        [
            "capture",
            "Core capture functionality."
        ],
        [
            "capture-css",
            "CSS related capture functionality."
        ],
        [
            "capture-js",
            "Javascript related capture functionality."
        ],
        [
            "capture-observer",
            "MutationObserver based capture."
        ],
        [
            "capture-insert",
            "DOM insert method rewrite based capture."
        ],
        [
            "timing",
            "Timed loading including DomReady, setTimeout, requestAnimationFrame and requestIdleCallback."
        ],
        [
            "responsive",
            "Responsive loading based on a Media Query."
        ],
        [
            "inview",
            "Timing based on element viewport visibility."
        ],
        [
            "api",
            "Public API and events."
        ],
        [
            "attr-config",
            "data-c HTML attribute based config via <script> element."
        ]
    ],
    "homepage": "https://github.com/style-tools/async-css",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/style-tools/async-css.git"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/style-tools/async-css/issues"
    },
    "engines": {
        "node": "~8"
    },
    "dependencies": {
        "cache-api-keyval": "^2.0.1",
        "console.table": "^0.10.0"
    },
    "devDependencies": {
        "builder": "^2.3.1",
        "chai": "^3.4.1",
        "commander": "^2.19.0",
        "console.table": "^0.10.0",
        "coveralls": "^2.11.4",
        "eslint": "^1.10.3",
        "eslint-config-defaults": "^9.0.0",
        "eslint-plugin-filenames": "^0.2.0",
        "grunt": "latest",
        "grunt-banner": "^0.6.0",
        "grunt-contrib-clean": "^2.0.0",
        "grunt-contrib-concat": "^1.0.1",
        "grunt-contrib-copy": "^1.0.0",
        "grunt-contrib-uglify": "latest",
        "grunt-iife": "latest",
        "grunt-sri": "^0.2.0",
        "guacamole": "^1.1.4",
        "http-server": "^0.8.5",
        "istanbul": "^0.4.0",
        "istanbul-instrumenter-loader": "^0.2.0",
        "js-combinatorics": "^0.5.4",
        "karma": "^0.13.15",
        "karma-chrome-launcher": "^0.2.2",
        "karma-coverage": "^0.5.3",
        "karma-firefox-launcher": "^0.1.7",
        "karma-ie-launcher": "^0.2.0",
        "karma-mocha": "^0.2.1",
        "karma-phantomjs-launcher": "^1.0.0",
        "karma-phantomjs-shim": "^1.1.2",
        "karma-requirejs": "^0.2.2",
        "karma-safari-launcher": "^0.1.1",
        "karma-sauce-launcher": "^0.3.0",
        "karma-sinon": "^1.0.5",
        "karma-spec-reporter": "0.0.24",
        "karma-webpack": "^1.7.0",
        "matchdep": "latest",
        "md5": "^2.2.1",
        "merge": "latest",
        "mocha": "^2.3.4",
        "mocha-chrome": "^1.1.0",
        "node-uuid": "^1.4.7",
        "phantomjs": "^2.1.3",
        "phantomjs-prebuilt": "^2.1.4",
        "request": "^2.88.0",
        "requirejs": "^2.1.22",
        "rimraf": "^2.4.4",
        "rowdy": "^0.4.0",
        "sauce-connect-launcher": "^0.14.0",
        "saucelabs": "^1.0.1",
        "selenium-standalone": "^6.15.4",
        "server-destroy": "^1.0.1",
        "sinon": "^1.17.6",
        "sinon-chai": "^2.8.0",
        "uglify-js": "^2.6.1",
        "webdriverio": "^4.0.3",
        "webpack": "^1.12.9",
        "zlib": "^1.0.5"
    },
    "scripts": {
        "lint-client": "eslint -c .eslintrc-client lib test/func/fixtures",
        "lint-client-test": "eslint -c .eslintrc-client-test test/client/*/spec test/client/*/main.js",
        "lint-server": "eslint -c .eslintrc-server *.js",
        "lint-server-test": "eslint -c .eslintrc-server-test test/func/*.js test/func/spec",
        "lint": "builder concurrent lint-client lint-client-test lint-server lint-server-test",
        "clean-cov": "rimraf coverage/func",
        "test-client-rjs": "karma start test/client/requirejs/karma.conf.js",
        "test-client-rjs-ci": "karma start --browsers PhantomJS,Firefox test/client/requirejs/karma.conf.coverage.js",
        "test-client-rjs-cov": "karma start test/client/requirejs/karma.conf.coverage.js",
        "test-client-wp": "karma start test/client/webpack/karma.conf.js",
        "test-client-wp-ci": "karma start --browsers PhantomJS,Firefox test/client/webpack/karma.conf.coverage.js",
        "test-client-wp-cov": "karma start test/client/webpack/karma.conf.coverage.js",
        "test-client": "builder concurrent --buffer test-client-rjs test-client-wp",
        "test-client-ci": "builder concurrent --buffer test-client-rjs-ci test-client-wp-ci",
        "test-client-cov": "builder concurrent --buffer test-client-rjs-cov test-client-wp-cov",
        "test-func": "mocha --colors --opts test/func/mocha.opts test/func/spec/",
        "test-func-cov": "mocha --colors --opts test/func/mocha-cov.opts test/func/spec/",
        "test-func-ci": "builder run test-func-cov",
        "test": "builder run test-client && builder run test-func",
        "test-ci": "builder concurrent --buffer test-client-ci test-func-ci",
        "setup-local-selenium": "selenium-standalone start >/dev/null 2>&1",
        "setup-local": "builder run setup-local-selenium",
        "test-func-local": "sleep 5 && echo \"Starting ${ROWDY_SETTINGS}\" && builder run test-func-cov",
        "setup-sauce-connect": "node_modules/sauce-connect-launcher/sc/*/bin/sc",
        "setup-sauce": "builder run setup-sauce-connect",
        "test-func-sauce": "sleep 20 && echo \"Starting ${ROWDY_SETTINGS}\" && builder run test-func-cov",
        "cov-report-func": "istanbul report --config .istanbul.func.yml --include 'coverage/func/data/coverage-*.json'",
        "check": "builder run lint && builder run test",
        "check-ci": "builder run lint && builder run test-ci",
        "server": "http-server -p 3001 .",
        "install-dev": "selenium-standalone install",
        "selenium-start": "selenium-standalone start",
        "selenium-stop": "selenium-standalone stop",
        "build": "rimraf dist && mkdir dist && uglifyjs lib/little-loader.js --stats --compress --mangle --output=dist/little-loader.min.js",
        "preversion": "builder run check",
        "version": "builder run build",
        "postversion": "git push && git push --tags && npm publish"
    }
}