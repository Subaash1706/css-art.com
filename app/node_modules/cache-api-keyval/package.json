{
    "name": "cache-api-keyval",
    "version": "2.0.1",
    "description": "Browser Cache API key/value store with +50MB capacity, expiration and JSON object data-type storage.",
    "author": {
        "name": "info@optimization.team",
        "email": "info@optimization.team",
        "web": "optimization.team"
    },
    "main": "./dist/cache-api-keyval-cjs.js",
    "module": "./dist/cache-api-keyval.mjs",
    "scripts": {
        "build": "del dist && rollup -c && npm run compress-iife && npm run create-compat && npm run compress-amd",
        "compress-iife": "uglifyjs --compress --mangle -o dist/cache-api-keyval-iife.min.js dist/cache-api-keyval-iife.js",
        "create-compat": "babel dist/cache-api-keyval-iife.js | uglifyjs --compress --mangle > dist/cache-api-keyval-iife-compat.min.js",
        "compress-amd": "uglifyjs --compress --mangle -o dist/cache-api-keyval-amd.min.js dist/cache-api-keyval-amd.js"
    },
    "keywords": [
        "cache",
        "cacheapi",
        "cache-api",
        "indexeddb",
        "cachestorage",
        "keyval",
        "localstorage",
        "store",
        "storage"
    ],
    "homepage": "https://github.com/optimalisatie/Cache-API-Key-Value-Store",
    "main": "cache-api-keyval.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/optimalisatie/Cache-API-Key-Value-Store.git"
    },
    "bugs": {
        "url": "https://github.com/optimalisatie/Cache-API-Key-Value-Store/issues"
    },
    "engines": {
        "node": "~0.10"
    },
    "dependencies": {},
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "del-cli": "^1.1.0",
        "rollup": "^0.56.5",
        "rollup-plugin-typescript2": "^0.12.0",
        "typescript": "^2.7.2",
        "uglify-es": "^3.3.9"
    }
}